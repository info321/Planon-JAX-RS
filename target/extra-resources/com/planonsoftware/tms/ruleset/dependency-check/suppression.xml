<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.1.xsd">
	<suppress>
   <notes><![CDATA[
    this will supress all the false positive with groupID .planon. or .planonsoftware. or .planon or .planonsoftware
   ]]></notes>
   <gav regex="true">[^:]*(\bplanon(software)?\b)([^:]*)(:[^:]*){1,}</gav>
   <cpe regex="true">.*</cpe>
</suppress>

</suppressions>